{
  "name": "puppet-<%= metadata.name %>",
  "version": "<%= metadata.version %>",
  "author": "Puppet Labs and StackForge Contributors",
  "summary": "Puppet module for OpenStack <%= metadata.name %>",
  "license": "Apache-2.0",
  "source": "git://github.com/stackforge/puppet-<%= metadata.name %>.git",
  "project_page": "https://launchpad.net/puppet-<%= metadata.name %>",
  "issues_url": "https://bugs.launchpad.net/puppet-<%= metadata.name %>",
  "description": "Installs and configures OpenStack <%= metadata.name %>.",
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.0.0 <5.0.0" },
    { "name": "stackforge/openstacklib", "version_requirement": ">=5.0.0 <6.0.0" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7"]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": ["20"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6.5","7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    }
  ]
}
